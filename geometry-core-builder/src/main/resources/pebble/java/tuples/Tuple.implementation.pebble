{#
 # This macro renders the implementation of Tuple of the tuple.
 #}
{%  macro Tuple( desc ) %}
    @Override
    public int size() {
        return {{ desc.dim }};
    }

    @Override
    public void getInto(TupleSet tupleSet) {
{%      for property in desc.properties %}
        tupleSet.set({{ property.index }}, {{ property.name }});
{%      endfor %}
    }
{%  endmacro %}

